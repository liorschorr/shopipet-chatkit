{
  "version": 2,
  "builds": [
    {
      "src": "api/chat.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/embed.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/chat.py"
    },
    {
      "src": "/web/embed.js",
      "dest": "web/embed.js"
    },
    {
      "src": "/",
      "dest": "api/chat.py"
    }
  ]
}
