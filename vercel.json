   {
  "version": 2,
  "builds": [
    { "src": "api/chat.py", "use": "@vercel/python@5.0.12" },
    { "src": "api/ping.py", "use": "@vercel/python@5.0.12" },
    { "src": "api/update_catalog.py", "use": "@vercel/python@5.0.12" }
  ],
  "routes": [
    { "src": "/api/update-catalog", "dest": "/api/update_catalog.py" },
    { "src": "/api/ping", "dest": "/api/ping.py" },
    { "src": "/api/(.*)", "dest": "/api/chat.py" }
  ],
  "crons": [
    { "path": "/api/update-catalog", "schedule": "0 3 * * *" }
  ]
}
