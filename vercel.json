{
  "version": 2,
  "builds": [
    {
      "src": "api/chat.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/chat.py"
    },
    {
      "src": "/web/(.*)",
      "dest": "web/$1"
    },
    {
      "src": "/openapi.json",
      "dest": "web/openapi.json"
    }
  ]
}
